<%= form_for @order, url: orders_create_path(@order.id), method: :post do |f|%>
     <%= f.label :address %><br />
     <%= f.text_field :address %>
     <% if @order.order_items.length > 0%>
        <%= f.submit "Complete Order"%>
     <% else%>
        <%= f.submit "Complete Order", disabled: true%>
     <% end%>
<% end %>


